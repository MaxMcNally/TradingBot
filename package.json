{
  "name": "myapp",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "build:api": "cd api && tsc",
    "build:core": "tsc",
    "start": "node dist/index.js",
    "start:api": "cd api && node dist/server.js",
    "start:core": "node dist/bot.js",
    "dev:server": "nodemon api/server.ts",
    "dev:client": "cd client && yarn dev",
    "dev": "yarn dev:server && yarn dev:client",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix && prettier --write .",
    "lint:api": "cd api && eslint . --ext .ts,.js",
    "lint:client": "cd client && yarn lint",
    "backtest": "npx ts-node src/backtest.ts",
    "cache:stats": "npx ts-node src/cache/cacheCLI.ts stats",
    "cache:cleanup": "npx ts-node src/cache/cacheCLI.ts cleanup",
    "cache:demo": "npx ts-node src/cache/example.ts",
    "bot": "npx ts-node src/bot.ts",
    "bot:cli": "npx ts-node src/cli/botCLI.ts",
    "bot:start": "npx ts-node src/cli/botCLI.ts start",
    "bot:status": "npx ts-node src/cli/botCLI.ts status",
    "bot:config": "npx ts-node src/cli/botCLI.ts config",
    "bot:db:init": "npx ts-node src/cli/botCLI.ts db init",
    "bot:user:create": "npx ts-node src/cli/botCLI.ts user create",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:api": "cd api && npm test",
    "test:client": "cd client && yarn test:run",
    "test:all": "yarn test:ci && yarn test:api && yarn test:client",
    "db:init": "npx ts-node api/initDb.ts",
    "db:reset": "rm -rf api/db/* db/* && yarn db:init",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:dev:down": "docker-compose -f docker-compose.dev.yml down",
    "ci:test": "yarn test:all && yarn lint",
    "ci:build": "yarn build && yarn build:api && cd client && yarn build",
    "ci:deploy": "yarn ci:test && yarn ci:build && yarn docker:build",
    "railway:deploy": "./scripts/deploy-railway.sh",
    "railway:setup": "railway login && railway project new",
    "railway:logs": "railway logs",
    "railway:status": "railway status"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.901.0",
    "@aws-sdk/node-http-handler": "^3.374.0",
    "@types/commander": "^2.12.5",
    "@types/cors": "^2.8.19",
    "@types/ws": "^8.18.1",
    "body-parser": "^1.20.2",
    "commander": "^14.0.1",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "csv-parser": "^3.2.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.19.2",
    "node-fetch": "^3.3.2",
    "sqlite3": "^5.1.6",
    "ws": "^8.18.3",
    "yahoo-finance2": "^2.13.3",
    "yargs": "^17.7.2",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@eslint/js": "^9.37.0",
    "@jest/globals": "^30.2.0",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.6.2",
    "@types/sqlite3": "^5.1.0",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "babel-loader": "^10.0.0",
    "eslint": "^9.37.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^6.1.1",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.3",
    "nodemon": "^3.0.1",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "webpack": "^5.102.0",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,json,css,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
